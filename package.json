{
  "name": "domaingenie",
  "version": "1.0.0",
  "description": "A fast and intelligent domain name generator from natural language",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx -y esbuild src/index.ts src/searchWorkerThread.ts --bundle --minify --outdir=dist --platform=neutral --format=esm --external:worker_threads",
    "postbuild": "cp dist/index.js docs/domaingenie.js && cp dist/searchWorkerThread.js docs/searchWorkerThread.js",
    "test": "npm run build && node --test"
  },
  "homepage": "https://namewiz.github.io/domaingenie",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namewiz/domaingenie.git"
  },
  "bugs": {
    "url": "https://github.com/namewiz/domaingenie/issues"
  },
  "author": "Justice Ogbonna <dev@justiceo.com>",
  "keywords": [
    "domain-name",
    "domain-generator",
    "name-generator",
    "natural-language",
    "ai-generator"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "domainstat": "^1.0.5",
    "stemmer": "^2.0.1",
    "synonyms": "^1.0.1",
    "word-list-json": "^0.2.0"
  }
}
