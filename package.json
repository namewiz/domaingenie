{
  "name": "domaingenie",
  "version": "0.0.8",
  "description": "A fast and intelligent domain name generator from natural language",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outdir=dist --platform=neutral --format=esm",
    "postbuild": "cp dist/index.js docs/domaingenie.js",
    "watch": "esbuild src/index.ts --bundle --outdir=dist --platform=neutral --format=esm --watch",
    "postwatch": "cp dist/index.js docs/domaingenie.js",
    "test": "npm run build && ava"
  },
  "homepage": "https://namewiz.github.io/domaingenie",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namewiz/domaingenie.git"
  },
  "bugs": {
    "url": "https://github.com/namewiz/domaingenie/issues"
  },
  "author": "Justice Ogbonna <dev@justiceo.com>",
  "keywords": [
    "domain-name",
    "domain-generator",
    "name-generator",
    "natural-language",
    "ai-generator"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "synonyms": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "esbuild": "^0.25.9",
    "typescript": "^5.3.3"
  }
}
